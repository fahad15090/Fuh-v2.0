function newQ(){
    var a = []
    var ret = $({})
    ret.add = function(job) {
        a.push(job)
        change()
        if (a.length!==1) return
        job(function consume(){
            a.shift() // trash it
            if (a.length)
                a[0](consume) // next
            else
                ret.trigger('empty')
            change()
        })
    }

    function change(){ ret.trigger({ type:'change', count:a.length }) }

    return ret
}//newQ

